<h1>Sign Up to ReachOut</h1>
<p>Find me in app/views/users/new.html.erb</p>

<div class="row">
<%= form_for(@user) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :password %>
<%= f.text_field :password %>
<%= f.label :confirm_passwrod %>
<%= f.text_field :confirm_passwrod %>

<%= f.submit "Create my account" %>
<% end%>
</div>